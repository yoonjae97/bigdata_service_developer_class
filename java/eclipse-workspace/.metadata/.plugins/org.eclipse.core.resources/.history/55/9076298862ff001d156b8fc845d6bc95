package kr.co.smhrd;

public class FileReader {

	public static void main(String[] args) {
		FileReaderEX in = null;
		
		try {
			in = new FileReaderEX("traffic.csv");
			int c;
			while ((c = in.read()) != -1) {
				System.out.print((char) c);
			}
			in.close();
		}
		catch (IOException e) {
			System.out.println();
		}

	}

}
