package kr.co.smhrd;

import java.io.FileReader;
import java.io.IOException;

public class prac {

	public static void main(String[] args) {
		FileReader one = null;
		
		try {
		one = new FileReader("c:\\Temp\\one.csv");
		int c;
		while ((c = one.read()) != -1) {
			System.out.println((char) c);
		}
		one.close();
		}
		catch (IOException e) {
			System.out.println("오류");
		}
	}

}
