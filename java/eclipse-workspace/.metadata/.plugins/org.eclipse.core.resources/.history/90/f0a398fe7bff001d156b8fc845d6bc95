package kr.co.smhrd;

import java.io.File;

public class FileRenameEX {

	public static void main(String[] args) {
		File file = new File("c:\\Temp\\test.txt");
		
		File newFile = new File("c:\\Temp\\test_2.txt");
		
		boolean result = file.renameTo(newFile);
		
		System.out.println(result);

	}

}
